<% layout('layouts/boilerplate') %>
<div class="grid h-screen px-4 bg-white place-content-center">
    <div class="text-center mb-40">
      <h1 class="font-black text-gray-200 text-9xl"><%= err.statusCode %></h1>
  
      <p class="text-2xl font-bold tracking-tight text-gray-900 sm:text-4xl">
        Uh-oh!
      </p>
  
      <p class="mt-4 text-gray-500"><%= err.message%></p>
  
      <a
        href="/"
        class="inline-block px-5 py-3 mt-6 text-sm font-medium text-white bg-[#32c0f0] rounded-lg hover:bg-[#32a4f0] focus:outline-none focus:ring"
      >
        Go Back Home
      </a>
    </div>
  </div>
